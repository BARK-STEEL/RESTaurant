<div class="row">
  <h3 class="page-header"><%= @server.name %></h3>





</div>

<div class="row admin-forms">
  <ul>

    <% @server.parties.each do |party| %>
      <form class="" action="/tickets/party" method="post">
        <button type="submit" name="party_id" value="<%= party.id  %>"><%= party.table_number %></button>
      </form>
    <% end %>

  </ul>
</div>
<div class="row main-button">

  <form class="admin" action="/tickets">
    <button type="submit">Back</button>
  </form>

  <form class="" action="/">
    <button type="submit">Main</button>
  </form>
</div>
